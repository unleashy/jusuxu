{
  "presets": [
    [
      "@babel/env",
      {
        "bugfixes": true,
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic",
        "importSource": ".",
        "useBuiltIns": true,
        "useSpread": true
      }
    ]
  ]
}
